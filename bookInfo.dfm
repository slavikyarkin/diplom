    Connection = Form3.con
    SQL.Strings = (
      'select r.*'
      '     , i.login '
      '  from mm.review r'
      '       join mm.identification i'
      '         on i.people_id = r.people_id'
      ' where r.book_id = :p_book_id'
      ' order by r.id desc')
    Active = True
